<coherence>
	
	<configurable-cache-factory-config>
		<class-name>com.oracle.coherence.environment.extensible.ExtensibleEnvironment</class-name>
		<init-params>
			<init-param>
				<param-type>string</param-type>
				<param-value system-property="tangosol.coherence.cacheconfig">cache-config.xml</param-value>
			</init-param>			
		</init-params>
	</configurable-cache-factory-config>

	<cluster-config>
		<member-identity>
			<cluster-name system-property="tangosol.coherence.cluster">Cache_Server_LOCAL</cluster-name>
		</member-identity>
		
		<multicast-listener>
			<time-to-live>0</time-to-live>
		</multicast-listener>

	</cluster-config>
</coherence>